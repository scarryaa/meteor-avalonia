{
  ".ts": {
    "Command": "/usr/bin/typescript-language-server",
    "Args": ["--stdio"],
    "InstallCommand": "npm install -g typescript-language-server typescript"
  },
  ".js": {
    "Command": "/usr/local/bin/typescript-language-server",
    "Args": ["--stdio"],
    "InstallCommand": "npm install -g typescript-language-server typescript"
  },
  ".py": {
    "Command": "/usr/local/bin/pyls",
    "Args": [],
    "InstallCommand": "pip install python-language-server"
  },
  ".cs": {
    "Command": "csharp-ls",
    "Args": [],
    "InstallCommand": "dotnet tool install --global csharp-ls"
  }
}